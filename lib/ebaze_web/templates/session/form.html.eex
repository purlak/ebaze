<%= form_for @changeset, @action, fn f -> %>
  <div class="form-group">
    <label>Username</label>
    <%= text_input f, :username %>
    <%= error_tag f, :username %>
  </div>

  <div class="form-group">
    <label>Password</label>
    <%= password_input f, :password %>
    <%= error_tag f, :password %>
  </div>

  <div class="form-group">
    <%= submit "Submit" %>
  </div>
<% end %>
